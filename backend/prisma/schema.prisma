generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int      @id @default(autoincrement())
  nombre       String
  email        String   @unique
  telefono     String?
  ciudad       String?
  empresa      String?
  creado_at    DateTime @default(now())
  actualizado_at DateTime @updatedAt
}
